			#funclist h6 { font-size: 1em; font-weight: bold; margin: 2px 0 0 0; padding: 0; }
			#funclist .icon {
				padding-left: 22px;
				background-repeat: no-repeat;
				line-height: 20px;
				background-position-y: 2px;
				background-position-x: 2px;
			}
			#funclist .ent { margin-left: 2px; overflow: hidden; white-space: nowrap; }
				#funclist .ent a { text-decoration: none; }
				#funclist .ent a:hover { text-decoration: underline; }
				#funclist .params { font-style: italic; color: gray; }
				#funclist .folder { background-image: url(data:image/png;base64,); }
				#funclist .module { background-image: url(data:image/png;base64,); }
				#funclist .function { background-image: url(data:image/png;base64,); }
				#funclist .table { background-image: url(data:image/png;base64,); }
				#funclist .value { background-image: url(data:image/png;base64,); }
	</style>
